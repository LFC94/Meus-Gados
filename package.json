{
  "name": "app-template",
  "version": "1.0.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "dev": "npx expo start",
    "check": "tsc --noEmit",
    "lint": "npx expo lint",
    "format": "prettier --write .",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "build:dev": "eas build --profile development --platform android",
    "build:preview": "eas build --profile preview --platform android",
    "build:prod": "eas build --profile production --platform android",
    "build:local": "npx expo prebuild --clean && cd android && gradlew assembleRelease --no-configuration-cache"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.6.0",
    "@react-native-firebase/app": "^23.8.2",
    "@react-native-firebase/auth": "^23.8.2",
    "@react-native-firebase/firestore": "^23.8.2",
    "@react-native-google-signin/google-signin": "^16.1.1",
    "@react-navigation/drawer": "^7.5.0",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/stack": "^7.6.13",
    "clsx": "^2.1.1",
    "expo": "^55.0.2",
    "expo-build-properties": "~55.0.9",
    "expo-constants": "~55.0.7",
    "expo-file-system": "~55.0.9",
    "expo-font": "~55.0.4",
    "expo-haptics": "~55.0.8",
    "expo-linking": "~55.0.7",
    "expo-notifications": "~55.0.10",
    "expo-router": "~55.0.2",
    "expo-sharing": "~55.0.10",
    "expo-splash-screen": "~55.0.9",
    "expo-status-bar": "~55.0.4",
    "expo-updates": "~55.0.11",
    "nativewind": "^4.2.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-native": "0.83.2",
    "react-native-css-interop": "^0.2.2",
    "react-native-gesture-handler": "~2.30.0",
    "react-native-gifted-charts": "^1.4.70",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "4.2.1",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.23.0",
    "react-native-svg": "15.15.3",
    "react-native-worklets": "0.7.2",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@types/node": "^22.19.3",
    "@types/react": "~19.1.17",
    "babel-preset-expo": "^54.0.10",
    "depcheck": "^1.4.7",
    "eslint": "^9.39.2",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.7.4",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.3"
  },
  "packageManager": "npm@10.2.3"
}
